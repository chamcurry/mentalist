<p class="text-center my-3">お気に入りリスト</p>
<%= form_with url: like_list_menbers_path,method: :get do %>
  <!--ジャンル検索-->
　<%= select_tag :genre_id,options_from_collection_for_select(Genre.all,:id,:soup,params[:genre_id]),{prompt: 'タグを選択'}%>
　<!--キーワード検索-->
　<%= text_field_tag :search,nil,placeholder: "キーワードを入力" %>
　<%= submit_tag "検索" %>
<% end %>
<%= render 'posts/post_list',posts: @posts,menber: @menber %>